{
    "env": {
        "browser": true,
        "es6": true,
        "jquery": true,
        "amd": true
    },
    "plugins": ["lodash"],
    "extends": ["eslint:recommended"],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly",
        "u" : false,
        "_": false
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "lodash/prefer-lodash-method" : "off"
    }
}